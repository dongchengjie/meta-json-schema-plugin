{
  "name": "meta-json-schema",
  "displayName": "Meta JSON Schema",
  "description": "A linter and snippet provider for Clash.Meta configuration.",
  "author": "dongchengjie",
  "contributors": [],
  "license": "MIT",
  "version": "1.0.4",
  "publisher": "ClashMeta",
  "bugs": "https://github.com/dongchengjie/meta-json-schema-plugin/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/dongchengjie/meta-json-schema-plugin"
  },
  "icon": "icon.png",
  "engines": {
    "npm": ">=7.0.0",
    "vscode": "^1.63.0"
  },
  "categories": [
    "Linters",
    "Snippets"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    },
    "virtualWorkspaces": true
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "keywords": [
    "clash",
    "schema",
    "linters",
    "snippets"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title:": "configuration",
      "properties": {
        "meta-json-schema.schemaURL": {
          "type": "string",
          "markdownDescription": "schema文件地址",
          "default": "https://fastly.jsdelivr.net/gh/dongchengjie/meta-json-schema@main/schemas/meta-json-schema.json"
        }
      }
    }
  },
  "extensionDependencies": [
    "redhat.vscode-yaml",
    "usernamehw.errorlens"
  ],
  "scripts": {
    "dev": "tsc -watch -p ./",
    "package": "tsc -p ./ && vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.63.0",
    "@vscode/vsce": "^2.26.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.6.8"
  }
}
