{
  "publisher": "ClashMeta",
  "name": "meta-json-schema",
  "displayName": "Meta JSON Schema",
  "description": "A linter and snippet provider for Clash.Meta configuration",
  "version": "1.0.2",
  "categories": [
    "Snippets",
    "Linters"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dongchengjie/meta-json-schema-plugin"
  },
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.67.0"
  },
  "extensionDependencies": [
    "redhat.vscode-yaml",
    "usernamehw.errorlens"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "dev": "tsc -watch -p ./",
    "package": "tsc -p ./ && vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "axios": "^1.6.8"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.67.0",
    "@vscode/vsce": "^2.26.0",
    "typescript": "^5.4.5"
  }
}
